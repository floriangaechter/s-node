<table cellpadding="0" cellspacing="0" width="100%">
 <tr>
  <td width="50%" style="height: 180px;" valign="top">
  <img id="map" src="{$XT_IMAGES}map/00.gif" alt="" usemap="#00" style="border-style:none" />
</td>
  <td valign="top">
  {foreach from=$AREAS item=AREA}
  <div id="area{$AREA.id}" style="display: none; visibility: hidden;" align="right">
   <table cellpadding="0" cellspacing="0" align="right" style="margin-left: 15px; margin-bottom: 15px;">
   <tr>
    <td>
   {image
       id=$AREA.image
       version=2
       title=$AREA.email
       alt=$AREA.email
       style="border: 1px solid #6F94B2"
   }
    </td>
   </tr>
   </table>
   <b>{$AREA.lastName} {$AREA.firstName}</b><br /><br />
   <a href="mailto:{$AREA.email}">{$AREA.email}</a><br />
   {$AREA.tel}
  </div>
  {/foreach}
  </td>
 </tr>
</table>
<table cellpadding="0" cellspacing="0" width="100%">
 <tr style="background-color: #D7E9F8;">
  <td class="basket" width="20%">{"Area"|translate}</td>
  <td class="basket" width="35%">{"Name"|translate}</td>
  <td class="basket" width="25%">{"E-Mail"|translate}</td>
  <td class="basket" width="20%">{"Telephone"|translate}</td>
 </tr>
{foreach from=$AREAS item=AREA}
 <tr onmouseover="showAreaDetails({$AREA.id}); this.style.backgroundColor='#D7E9F8'" onmouseout="this.style.backgroundColor='transparent'">
  <td class="basket_borderbottom"><b>{$AREA.title}</b></td>
  <td class="basket_borderbottom">{$AREA.lastName} {$AREA.firstName}</td>
  <td class="basket_borderbottom"><a href="mailto:{$AREA.email}">{$AREA.email}</a></td>
  <td class="basket_borderbottom">{$AREA.tel}</td>
 </tr>
{/foreach}
</table>
{literal}
<script language="JavaScript"><!--

var last = 0;

function showAreaDetails(id){
    if(last > 0){
        document.getElementById('area' + last).style.visibility='hidden';
        document.getElementById('area' + last).style.display='none';
    }
    document.getElementById('area' + id).style.visibility='visible';
    document.getElementById('area' + id).style.display='block';
    document.getElementById('map').src='{$XT_IMAGES}map/' + id + '.gif';
    last = id;
}
//-->
</script>
{/literal}
